{
  "name": "fintok_server",
  "version": "1.0.0",
  "description": "backend for stock browser app",
  "main": "./bin/server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node ./bin/server.js",
    "start:dev": "concurrently \"nodemon --ignore '../frontend/*'\" \"npm run client\"",
    "client": "cd ../frontend && npm run start",
    "install": "cd ../frontend && npm install"
  },
  "keywords": [
    "fetch",
    "sequelize"
  ],
  "author": "peter blank",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "crypto-js": "^4.0.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "node-fetch": "^2.6.1",
    "pg": "^8.3.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "morgan": "^1.10.0",
    "prettier": "^2.1.2",
    "sequelize-cli": "^6.2.0"
  }
}
